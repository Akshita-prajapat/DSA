class Solution {
  public:
    vector<int> removeDuplicates(vector<int> &arr) {
        int n = arr.size();
        int i =0;
        if(n == 0) return {};
        for(int j =1;j< n;j++){
            if(arr[i] != arr[j]){
                i++;
                arr[i] = arr[j];
            }
        }
        vector<int> result;
        for(int k = 0; k<=i; k++){
            result.push_back(arr[k]);
        }
        return result;
    }
};
