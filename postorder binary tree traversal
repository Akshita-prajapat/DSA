class Solution {
  public:
    vector<int> postOrder(Node* root) {
        vector<int> ans;
        postorder(root , ans);
        return ans;
    }
    void postorder(Node* root , vector<int> &ans){
        if(root == NULL) return ;
        postorder(root -> left , ans);
        postorder(root -> right , ans);
        ans.push_back(root -> data);
    }
};
