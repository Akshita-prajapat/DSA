class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n = nums.size();
        int tSum = n*(n+1)/2; // sum of all integers
        int sum = 0;
        int ans;
        for(int i= 0; i<n ;i++){
            sum += nums[i];
            ans = tSum - sum;
        }
        return ans;
    }
};
