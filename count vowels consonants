void checkString(string s) {
    int v = 0; 
    int c = 0; 
    string vowels = "aeiou"; 

    for (char ch : s) { 
        if (vowels.find(ch) != string::npos) {
            v++; 
        } else if (ch >= 'a' && ch <= 'z') {
            c++; 
        }
    }

    if (v > c)
        cout << "Yes";
    else if (c > v)
        cout << "No";
    else
        cout << "Same";

    cout << endl;
}
