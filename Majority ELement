class Solution {
public:
    int majorityElement(vector<int>& arr) {
        int n = arr.size();
        int count= 0;
        int el = arr[0];
        for(int i = 0 ;i<n ;i++){
            if(count == 0){
            el = arr[i];
            count++;
            }
            else if(arr[i] == el) count++;
            else count--;
         }
         return el;
    }
};
