
class myQueue {
    Node* front;
    Node* rear;
    int sz;

  public:
    myQueue() {
        // Initialize your data members
        front = rear = NULL;
        sz = 0;
    }

    bool isEmpty() {
        // check if the queue is empty
        return front == NULL;
    }

    void enqueue(int x) {
        // Adds an element x at the rear of the queue
        Node* newNode = new Node(x);
        if(rear == NULL){
            front = rear = newNode;
            sz++;
            return;
        }
        rear -> next  = newNode;
        rear = newNode;
        sz++;
    }

    void dequeue() {
        // Removes the front element of the queue
        if(isEmpty()) return;
        Node* temp = front;
        front = front -> next;
        if(front == NULL) rear = NULL;
         delete temp;
         sz--;
    }

    int getFront() {
        // Returns the front element of the queue
        // If queue is empty, return -1
        if (isEmpty()) return -1;
        return front->data;
    }

    int size() {
        // Returns the current size of the queue.
        return sz;
    }
};
